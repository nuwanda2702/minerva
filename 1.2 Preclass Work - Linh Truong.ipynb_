    "def bubbleSort(A):\n",
    "    for i in range(len(A)):\n",
    "        for j in (len(A)-1,1,-1):\n",
    "            if A[j] < A[j-1]:\n",
    "                sortingnum = A[j]\n",
    "                A[j] = A[j-1]\n",
    "                A[j-1] = sortingnum\n",
    "    return(A)\n",
    "    raise NotImplementedError()\n",
    "A=[9,7,10,100]\n",
    "bubbleSort(A)\n",
    "print(A)"
